class Solution {
public:
	string simplifyPath(string path){
      vector<string> result;
      for(int i=0;i<path.length();i++)
      {
    	  if(path[i]=='')
      }
    }
};

int main()
{
  Solution s;
  string ss="a  b  ";
  string res=s.simplifyPath(ss);
  cout<< "res=" <<res <<endl;
  return 0;
}








