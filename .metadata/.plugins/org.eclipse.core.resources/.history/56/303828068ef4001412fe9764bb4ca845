#include <stdio.h>
#include <iostream>
#include<algorithm>

using namespace std;


class Solution
{
public:
	  bool isPalindrome(string s)
	  {
		 int len= s.length();
		 cout<< "len=" << len << endl;
	     return true;
	  }

};
void main()
{
    Solution ss ;
    string s="i am i" ;
    ss.isPalindrome(s);

}


